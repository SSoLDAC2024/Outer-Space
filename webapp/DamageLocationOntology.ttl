###  http://www.ldac.org/damage-location/hasBoundingBox
:hasBoundingBox rdf:type owl:ObjectProperty ;
                rdfs:domain brot:Bridge ,
                            brot:Component ;
                rdfs:range arp:BoundingBox ;
                rdfs:comment "An object has a bounding box."@en ;
                rdfs:label "hasBoundingBox"@en .


###  http://www.ldac.org/damage-location/hasDocument
:hasDocument rdf:type owl:ObjectProperty ;
             rdfs:domain brot:Component ;
             rdfs:range foaf:Document ;
             rdfs:comment "a component has a document."@en ;
             rdfs:label "hasDocument"@en .


###  http://www.ldac.org/damage-location/hasGrid
:hasGrid rdf:type owl:ObjectProperty ;
         rdfs:domain sf:Surface ;
         rdfs:range :Grid ;
         rdfs:comment "a surface has a grid."@en ;
         rdfs:label "hasGrid"@en .


###  http://www.ldac.org/damage-location/hasPart
:hasPart rdf:type owl:ObjectProperty ;
         rdfs:domain :Grid ;
         rdfs:range :PartOfGrid ;
         rdfs:comment "a grid has a part."@en ;
         rdfs:label "hasPart"@en .


###  http://www.ldac.org/damage-location/hasPartOnGrid
:hasPartOnGrid rdf:type owl:ObjectProperty ;
               rdfs:domain foaf:Document ,
                           dot:DamageArea ;
               rdfs:range :PartOfGrid ;
               rdfs:comment "Something is defined by a part on a grid."@en ;
               rdfs:label "hasPartOnGrid"@en .


###  http://www.ldac.org/damage-location/hasSurface
:hasSurface rdf:type owl:ObjectProperty ;
            rdfs:domain arp:BoundingBox ;
            rdfs:range sf:Surface ;
            rdfs:comment "A bounding box has a surface."@en ;
            rdfs:label "hasSurface"@en .


###  http://www.ldac.org/damage-location/isParallelTo
:isParallelTo rdf:type owl:ObjectProperty ,
                       owl:InverseFunctionalProperty ;
              rdfs:domain sf:Surface ;
              rdfs:range sf:Surface ;
              rdfs:comment "A surface is parallel to another surface."@en ;
              rdfs:label "isParallelTo"@en .


